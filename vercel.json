{
  "version": 2,
  "name": "opaque-conduit",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bootstrap",
      "dest": "/api/bootstrap.js"
    },
    {
      "src": "/api/handshake",
      "dest": "/api/handshake.js"
    },
    {
      "src": "/api/exchange",
      "dest": "/api/exchange.js"
    },
    {
      "src": "/api/get-payload",
      "dest": "/api/get-payload.js"
    }
  ]
}